{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Default CMake",
        "type": "cppdbg",
        "request": "launch",
        "program": "${workspaceFolder}/build/gurobi_app",
        "args": [],
        "stopAtEntry": false,
        "cwd": "${workspaceFolder}",
        "environment": [
          {
            "name": "LD_LIBRARY_PATH",
            "value": "/opt/gurobi1201/linux64/lib"
          }
        ],
        "preLaunchTask": "build",
        "externalConsole": false,
        "MIMode": "gdb",
        "setupCommands": [
          {
            "description": "Enable pretty-printing for gdb",
            "text": "-enable-pretty-printing",
            "ignoreFailures": true
          }
        ],
        "miDebuggerPath": "/usr/bin/gdb"
      }
    ]
  }
